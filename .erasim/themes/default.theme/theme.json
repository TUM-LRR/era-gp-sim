        "snapshots": {
            "button": {
                "background": "#FFFFFF",
                "paddingLeft": 10,
                "fontSize": 14,
                "paddingRight": 20
            }
        },
        "h1": {
            "padding": 10,
            "color": "#212121",
            "fontSize": 16
        },
        "hr": {
            "color": "#BDBDBD",
            "height": 1
        }
    }
