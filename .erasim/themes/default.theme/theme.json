{
    "memory": {
        "cell": {
            "borderRadius": 3,
            "width": 95,
            "fontSize": 12,
            "borderColor": "#BDBDBD",
            "color": "#212121",
            "background": "transparent",
            "height": 24
        },
        "divider": {
            "color": "#BDBDBD",
            "width": 1
        },
        "header": {
            "fontSize": 12,
            "fontWeight": "bold",
            "remove": {
                "fontSize": 16,
                "marginBottom": 0,
                "color": "#FF4081",
                "background": "transparent",
                "marginLeft": 0,
                "fontWeight": "bold"
            },
            "background": "red",
            "button": {
                "width": 70
            },
            "height": 25
        },
        "add": {
            "fontSize": 14,
            "marginBottom": 2,
            "marginRight": 0,
            "color": "#FF4081",
            "background": "transparent",
            "fontWeight": "bold"
        },
        "address": {
            "width": 90,
            "fontSize": 12,
            "color": "#212121",
            "background": "transparent",
            "marginLeft": -10,
            "fontWeight": "normal"
        }
    },
    "window": {
        "background": "white",
        "width": 0.9,
        "initialWidth": 300,
        "height": 0.9,
        "initialHeight": 350
    },
    "editor": {
        "selection": "#2d2d91",
        "lineHighlight": {
            "border": {
                "color": "#1ab2b2b2"
            },
            "background": "#1ae8e8e8"
        },
        "issue": {
            "warning": {
                "lightColor": "#10ffb90a",
                "darkColor": "#ffe39d"
            },
            "information": {
                "lightColor": "#100075ff",
                "darkColor": "#99c8ff"
            },
            "error": {
                "lightColor": "#10ff5000",
                "darkColor": "#ffb998"
            }
        },
        "executionLineHighlight": {
            "border": {
                "color": "transparent"
            },
            "background": "#1a2196F3"
        },
        "macro": {
            "background": "#f5f5f5"
        },
        "keywords": {
            "register": {
                "color": "#f59332"
            },
            "instruction": {
                "color": "#2196F3"
            },
            "immediate": {
                "color": "#a86c0d"
            },
            "label": {
                "color": "#FF4081"
            },
            "comment": {
                "color": "green"
            }
        },
        "color": "black",
        "background": "white",
        "sidebar": {
            "background": "#e8e8e8",
            "lineNumber": {
                "color": "gray"
            }
        },
        "breakpoint": {
            "color": "#FF4081"
        }
    },
    "createProject": {
        "marginTop": 10,
        "width": 280,
        "name": {
            "length": 80
        },
        "button": {
            "marginTop": 10,
            "width": 160,
            "background": "#2196F3"
        },
        "section": {
            "borderWidth": 1,
            "fontSize": 20,
            "borderColor": "#BDBDBD",
            "height": 50,
            "margin": 20
        }
    },
    "register": {
        "marginRight": 10,
        "selector": {
            "width": 15,
            "fontSize": 14,
            "color": "#FF4081",
            "background": "transparent",
            "radius": 1,
            "height": 23
        },
        "content": {
            "border": {
                "color": "#BDBDBD",
                "width": 1
            },
            "disabled": {
                "color": "#757575",
                "background": "#f2f2f2"
            },
            "fontSize": 12,
            "highlighted": {
                "background": "#FF4081"
            },
            "margin": 4,
            "color": "#212121",
            "background": "white",
            "radius": 2,
            "height": 25
        },
        "overflow": "auto",
        "background": "transparent",
        "marginLeft": -5,
        "label": {
            "color": "#212121",
            "fontSize": 12,
            "fontWeight": "bold"
        },
        "height": 40
