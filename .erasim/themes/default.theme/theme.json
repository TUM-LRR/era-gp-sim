{
    "toolbar": {
        "margin": 20,
        "background": "white"
    },
    "input": {
        "tabBar": {
            "border": {
                "color": "#BDBDBD",
                "width": 1
            },
            "background": "white"
        }
    },
    "memory": {
        "divider": {
            "color": "#BDBDBD",
            "width": 1
        },
        "address": {
            "background": "transparent",
            "color": "#212121",
            "marginLeft": -10,
            "width": 90,
            "fontWeight": "normal",
            "fontSize": 12
        },
        "cell": {
            "minWidth": 110,
            "borderRadius": 3,
            "borderColor": "#BDBDBD",
            "height": 24,
            "background": "transparent",
            "color": "#212121",
            "fontSize": 12
        },
        "add": {
            "marginBottom": 2,
            "background": "transparent",
            "color": "#FF4081",
            "marginRight": 2,
            "width": 10,
            "fontWeight": "bold",
            "fontSize": 14
        },
        "header": {
            "height": 25,
            "remove": {
                "marginBottom": 0,
                "background": "transparent",
                "color": "#FF4081",
                "marginLeft": 0,
                "fontWeight": "bold",
                "fontSize": 16
            },
            "background": "transparent",
            "button": {
                "width": 70
            },
            "fontWeight": "bold",
            "fontSize": 12
        }
    },
    "window": {
        "initialHeight": 350,
        "width": 0.9,
        "height": 0.9,
        "initialWidth": 300,
        "background": "white"
    },
    "button": {
        "radius": 4,
        "border": {
            "color": "#82c4f8",
            "width": 2
        },
        "height": 40,
        "disabled": {
            "border": {
                "color": "#d7d7d7"
            },
            "color": "#212121",
            "background": "#BDBDBD"
        },
        "color": "#FFFFFF",
        "fontSize": 16
    },
    "editor": {
        "sidebar": {
            "lineNumber": {
                "color": "gray"
            },
            "background": "#e8e8e8"
        },
        "breakpoint": {
            "color": "#FF4081"
        },
        "background": "white",
        "selection": "#2d2d91",
        "color": "black",
        "keywords": {
            "register": {
                "color": "#f59332"
            },
            "label": {
                "color": "#FF4081"
            },
            "comment": {
                "color": "green"
            },
            "immediate": {
                "color": "#a86c0d"
            },
            "instruction": {
                "color": "#2196F3"
            }
        },
        "executionLineHighlight": {
            "border": {
                "color": "transparent"
            },
            "background": "#1a2196F3"
        },
        "issue": {
            "information": {
                "lightColor": "#100075ff",
                "darkColor": "#99c8ff"
            },
            "error": {
                "lightColor": "#10ff5000",
                "darkColor": "#ffb998"
            },
            "warning": {
                "lightColor": "#10ffb90a",
                "darkColor": "#ffe39d"
            }
        },
        "macro": {
            "background": "#f5f5f5"
        },
        "lineHighlight": {
            "border": {
                "color": "#1ab2b2b2"
            },
            "background": "#1ae8e8e8"
        }
    },
    "register": {
        "marginLeft": -5,
        "overflow": "auto",
        "height": 40,
        "background": "transparent",
        "label": {
            "color": "#212121",
            "fontWeight": "bold",
            "fontSize": 12
        },
        "content": {
            "radius": 2,
            "border": {
                "color": "#BDBDBD",
                "width": 1
            },
            "height": 25,
            "background": "white",
            "color": "#212121",
            "disabled": {
                "color": "#757575",
                "background": "#f2f2f2"
            },
            "margin": 4,
            "fontSize": 12,
            "highlighted": {
                "background": "#FF4081"
            }
        },
        "marginRight": 10,
        "selector": {
            "radius": 1,
            "height": 23,
            "background": "transparent",
            "color": "#FF4081",
            "width": 15,
            "fontSize": 14
        }
    },
    "createProject": {
        "button": {
            "width": 160,
            "marginTop": 10,
            "background": "#2196F3"
        },
        "name": {
            "length": 80
        },
        "width": 280,
        "section": {
            "borderWidth": 1,
            "borderColor": "#BDBDBD",
            "margin": 20,
            "fontSize": 20,
            "height": 50
        },
        "marginTop": 10
    },
    "settings": {
        "section": {
            "height": 95,
            "paddingBottom": 5
        },
        "hr": {
            "color": "#BDBDBD",
            "height": 1
        },
        "h1": {
            "color": "#212121",
            "padding": 10,
            "fontSize": 16
        },
        "h2": {
            "marginBottom": 10,
            "color": "#757575",
            "marginTop": 8,
            "fontSize": 13
        },
        "snapshots": {
            "button": {
                "paddingLeft": 10,
                "paddingRight": 20,
                "fontSize": 14,
                "background": "#FFFFFF"
            }
        },
        "paddingTop": 15
    }
}
