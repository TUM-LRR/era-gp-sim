        "marginTop": 10,
        "name": {
            "length": 80
        },
        "button": {
            "background": "#2196F3",
            "marginTop": 10,
            "width": 160
        },
        "width": 280
    },
    "register": {
        "selector": {
            "radius": 1,
            "height": 23,
            "color": "#FF4081",
            "fontSize": 14,
            "background": "transparent",
            "width": 15
        },
        "label": {
            "fontSize": 12,
            "fontWeight": "bold",
            "color": "#212121"
        },
        "overflow": "auto",
        "marginLeft": -5,
        "background": "transparent",
        "content": {
            "radius": 2,
            "margin": 4,
            "color": "#212121",
            "disabled": {
                "background": "#f2f2f2",
                "color": "#757575"
            },
            "border": {
                "color": "#BDBDBD",
                "width": 1
            },
            "fontSize": 12,
            "background": "white",
            "highlighted": {
                "background": "#FF4081"
            },
            "height": 25
        },
        "marginRight": 10,
        "height": 40
    },
    "input": {
        "tabBar": {
            "border": {
                "color": "#BDBDBD",
                "width": 1
            },
            "background": "white"
        }
    }
}
