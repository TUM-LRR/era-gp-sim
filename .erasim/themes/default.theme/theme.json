    "button": {
        "border": {
            "color": "#82c4f8",
            "width": 2
        },
        "disabled": {
            "color": "#212121",
            "background": "#BDBDBD",
            "border": {
                "color": "#d7d7d7"
            }
        },
        "fontSize": 16,
        "color": "#FFFFFF",
        "radius": 4,
        "height": 40
